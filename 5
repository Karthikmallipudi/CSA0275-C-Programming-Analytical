#include <stdio.h>

void printCombination(int arr[], int data[], int start, int end, int index, int r) {
    if (index == r) {
        for (int i = 0; i < r; i++) {
            printf("%d ", data[i]);
        }
        printf("\n");
        return;
    }

    for (int i = start; i <= end && end - i + 1 >= r - index; i++) {
        data[index] = arr[i];
        printCombination(arr, data, i + 1, end, index + 1, r);

        while (arr[i] == arr[i + 1]) {
            i++;
        }
    }
}

void findCombinations(int arr[], int n, int r) {
    int data[r];
    printCombination(arr, data, 0, n - 1, 0, r);
}

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int k = 3;

    printf("Distinct combinations of length %d:\n", k);
    findCombinations(arr, n, k);

    return 0;
}
